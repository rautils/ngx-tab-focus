{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-tab-focus": [
        "dist/ngx-tab-focus/ngx-tab-focus",
        "dist/ngx-tab-focus"
      ],
      "ngx-image-fallback": [
        "dist/ngx-image-fallback/ngx-image-fallback",
        "dist/ngx-image-fallback"
      ]
    },
    "types": [
      "jest"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}