{
	"name": "rautils",
	"version": "0.0.1",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"clean:dist": "rimraf ./dist",
		"clean:node_modules": "rimraf ./node_modules",
		"clean:all": "npm run clean:dist && npm run clean:node_modules",
		"bootstrap": "npm install",
		"build:prod": "ng build --prod",
		"build:ci": "npm run clean && npm run test && npm run build:prod",
		"build:demo": "ng build rautils-demo --prod --base-href='/rautils/'",
		"test": "ng test",
		"test:coverage": "ng test --coverage",
		"test:watch": "ng test --watch",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"postinstall": "ngcc",
		"rautility": "ng g @rautils/schematics:rautility"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~9.1.1",
		"@angular/common": "~9.1.1",
		"@angular/compiler": "~9.1.1",
		"@angular/core": "~11.0.5",
		"@angular/forms": "~9.1.1",
		"@angular/platform-browser": "~9.1.1",
		"@angular/platform-browser-dynamic": "~9.1.1",
		"@angular/router": "~9.1.1",
		"angular-cli-ghpages": "^0.6.2",
		"rxjs": "~6.5.4",
		"tslib": "^1.10.0",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-builders/jest": "^9.0.1",
		"@angular-devkit/build-angular": "~0.901.1",
		"@angular-devkit/build-ng-packagr": "~0.901.1",
		"@angular/cli": "~9.1.1",
		"@angular/compiler-cli": "~9.1.1",
		"@angular/language-service": "~9.1.1",
		"@rautils/schematics": "^1.0.0",
		"@types/jest": "^25.2.1",
		"@types/node": "^12.11.1",
		"codelyzer": "^5.1.2",
		"glob": "^7.1.6",
		"jest": "^25.3.0",
		"jest-preset-angular": "^8.1.3",
		"ng-packagr": "^9.0.0",
		"protractor": "~5.4.3",
		"rimraf": "^3.0.2",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"typescript": "~3.8.3"
	}
}
