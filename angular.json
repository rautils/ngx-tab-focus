{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"@rautils/ngx-tab-focus": {
			"projectType": "library",
			"root": "projects/ngx-tab-focus",
			"sourceRoot": "projects/ngx-tab-focus/src",
			"prefix": "tab-focus",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-ng-packagr:build",
					"options": {
						"tsConfig": "projects/ngx-tab-focus/tsconfig.lib.json",
						"project": "projects/ngx-tab-focus/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/ngx-tab-focus/tsconfig.lib.prod.json"
						}
					}
				},
				"test": {
					"builder": "@angular-builders/jest:run",
					"options": {
						"tsConfig": "projects/ngx-tab-focus/tsconfig.spec.json"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/ngx-tab-focus/tsconfig.lib.json",
							"projects/ngx-tab-focus/tsconfig.spec.json"
						],
						"exclude": ["**/node_modules/**"]
					}
				}
			}
		},
		"@rautils/ngx-tab-focus-demo": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/ngx-tab-focus-demo",
			"sourceRoot": "projects/ngx-tab-focus-demo/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/ngx-tab-focus-demo",
						"index": "projects/ngx-tab-focus-demo/src/index.html",
						"main": "projects/ngx-tab-focus-demo/src/main.ts",
						"polyfills": "projects/ngx-tab-focus-demo/src/polyfills.ts",
						"tsConfig": "projects/ngx-tab-focus-demo/tsconfig.app.json",
						"aot": true,
						"assets": [
							"projects/ngx-tab-focus-demo/src/favicon.ico",
							"projects/ngx-tab-focus-demo/src/assets"
						],
						"styles": [
							"projects/ngx-tab-focus-demo/src/styles.scss"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "projects/ngx-tab-focus-demo/src/environments/environment.ts",
									"with": "projects/ngx-tab-focus-demo/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					}
				},
				"deploy": {
					"builder": "angular-cli-ghpages:deploy",
					"options": {}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "ngx-tab-focus-demo:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "ngx-tab-focus-demo:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "ngx-tab-focus-demo:build"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/ngx-tab-focus-demo/tsconfig.app.json",
							"projects/ngx-tab-focus-demo/tsconfig.spec.json",
							"projects/ngx-tab-focus-demo/e2e/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
			}
		}
	},
	"cli": {
		"analytics": false
	},
	"defaultProject": "@rautils/ngx-tab-focus"
}
